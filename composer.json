{
    "name": "silasyudi/optional",
    "description": "Portability of Java's java.util.Optional<T> class to PHP.",
    "keywords": ["optional", "java"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Yudi",
            "email": "syudik12@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml.dist --testdox"
    },
    "autoload": {
        "psr-4": {
            "SilasYudi\\Optional\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilasYudi\\Optional\\Tests\\": "tests/"
        }
    }
}
